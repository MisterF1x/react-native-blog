{
  "expo": {
    "name": "Blog-RN",
    "slug": "Blog-RN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Blog-RN to access your photos.",
          "savePhotosPermission": "Allow Blog-RN to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow the Blog-RN to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Blog-RN to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The Blog-RN accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Blog-RN to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Blog-RN to save photos."
      }
    },
    "android": {
      "package": "com.blog.test",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config":{
        "googleMaps":{
          "apiKey": "AIzaSyCj-nyOn08gX9SpMyHG6pD4BEZsAQeKq9k"
        }
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "70158a4f-4ffc-4dcd-adee-ba421cf6f304"
      }
    }
  }
}
